DROP TABLE addresses;

CREATE TABLE students(
id INTEGER PRIMARY KEY,
studentNumber TEXT,
firstName TEXT,
lastName TEXT,
yearLevel TEXT,
program TEXT,
email TEXT,
datecreated TEXT);

CREATE TABLE courses(
id INTEGER PRIMARY KEY,
courseNumber TEXT,
title TEXT,
specialization TEXT,
credit TEXT,
active TEXT);

CREATE TABLE grades(
id INTEGER PRIMARY KEY,
courseId TEXT,
studentId TEXT,
value TEXT,
term TEXT,
year TEXT);

CREATE TABLE users(
id INTEGER PRIMARY KEY,
firstName TEXT,
lastName TEXT,
position TEXT,
email TEXT,
username TEXT,
password TEXT);

CREATE TABLE profiles(
id INTEGER PRIMARY KEY,
dateCreated TEXT,
initialDate TEXT,
finalDate TEXT,
initialResultRank TEXT,
initialResult1 TEXT,
initialResult2 TEXT,
initialResult3 TEXT,
finalResultRank TEXT,
finalResult TEXT,
finalResult1 TEXT,
finalResult2 TEXT,
finalResult3 TEXT,
status TEXT,
studentId TEXT,
userId TEXT,
sdExam TEXT,
dsExam TEXT,
msExam TEXT,
sdInterview TEXT,
dsInterview TEXT,
msInterview TEXT);

